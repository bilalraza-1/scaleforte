<div class="contact-form">
	<div class="form-wrapper">
		<div class="form-details">
			<h2>Request a Call</h2>
			<p>
				This moderate and humid climate provides<br />
				the massive amounts
			</p>
		</div>
		<div class="form-section">
			<%= form_with(model: @request, url: request_form_submissions_create_path, method: "POST") do |form| %>
				<div class="form-group">
					<div class="form-item">
						<%= form.label :name%>
						<%= form.text_field :name, class: "contact-form-input", required: true %>
					</div>
					<div class="form-item">
						<%= form.label :email%>
						<%= form.text_field :email, class: "contact-form-input", required: true %>
					</div>
				</div>

				<div class="form-group">
					<div class="form-item">
						<%= form.label :country%>
						<%= form.select :country, options_for_select(@countries, ["United Kingdom", "GB"]), {}, { :class => 'contact-form-input'} %>
					</div>
					<div class="form-item">
						<%= form.label :phone%>
						<%= form.text_field :phone, class: "contact-form-input" %>
					</div>
				</div>

				<div class="form-group">
					<div class="form-item">
						<%= form.label :comment%>
						<%= form.text_area :comment %>
					</div>
				</div>

				<div>
					<%= form.submit "SUBMIT", class: "submit-button" %>
				</div>

			<% end %>
		</div>
	</div>
</div>

